            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:layout_width="65dp"
            android:layout_height="51dp" android:id="@+id/profile_home_button"
            app:icon="@android:drawable/ic_menu_my_calendar"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="664dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="330dp"
            android:layout_marginEnd="16dp" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="Welcome, #Name"
            android:layout_width="113dp"
            android:layout_height="81dp" android:id="@+id/textView2"
            android:fontFamily="@font/amarante" android:textSize="24sp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="21dp"
