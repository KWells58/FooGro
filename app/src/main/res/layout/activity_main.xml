            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="18dp"
            android:layout_marginEnd="280dp" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="Choose Your Category to Compare Prices"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            android:fontFamily="@font/amarante" android:textSize="24sp"
