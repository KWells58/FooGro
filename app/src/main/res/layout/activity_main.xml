            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="456dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="Grocery"
            android:layout_width="396dp"
            android:layout_height="213dp" android:id="@+id/grocery_button"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="377dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="195dp" app:layout_constraintVertical_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="9dp"
            app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>