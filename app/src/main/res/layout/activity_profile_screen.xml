<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Profile_Screen">
    <edu.floridapoly.mobiledeviceapps.fall23.foogro_1.Navigation_Bar
            android:id="@+id/navigation_bar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="Profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView6"
            android:fontFamily="@font/amarante" android:textSize="34sp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="34dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="654dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="31dp"
            android:layout_marginEnd="283dp" app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="Settings"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/settings_button"
            android:fontFamily="@font/amarante"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="435dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="248dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="160dp"
            android:layout_marginEnd="153dp" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="#firstname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView7"
            android:fontFamily="@font/amarante" android:textSize="20sp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="51dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="654dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="267dp"
            android:layout_marginEnd="45dp" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="#lastname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView8"
            android:fontFamily="@font/amarante" android:textSize="20sp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="102dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="603dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="267dp"
            android:layout_marginEnd="50dp" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="#address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView9"
            android:fontFamily="@font/amarante" android:textSize="20sp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="169dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="536dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="267dp"
            android:layout_marginEnd="64dp" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="#personal_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView10"
            android:fontFamily="@font/amarante" android:textSize="24sp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="340dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="361dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="128dp"
            android:layout_marginEnd="121dp" app:layout_constraintEnd_toEndOf="parent"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:srcCompat="@tools:sample/avatars" android:id="@+id/imageView"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="94dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="509dp" android:layout_marginEnd="267dp"
            app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>