<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Settings_Screen">
    <edu.floridapoly.mobiledeviceapps.fall23.foogro_1.Navigation_Bar
            android:id="@+id/navigation_bar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

    <Button
            android:text="Logout"
            android:layout_width="324dp"
            android:layout_height="57dp" android:id="@+id/logout_button"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="35dp"
            android:layout_marginEnd="52dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="25dp"
            app:layout_constraintTop_toBottomOf="@+id/browsing_history_button" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="240dp" android:fontFamily="@font/amarante"/>
    <Button
            android:text="Browsing History"
            android:layout_width="324dp"
            android:layout_height="57dp" android:id="@+id/browsing_history_button"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="35dp"
            android:layout_marginEnd="52dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="25dp"
            app:layout_constraintTop_toBottomOf="@+id/saved_items_button" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="323dp" android:fontFamily="@font/amarante"/>
    <Button
            android:text="Saved Items"
            android:layout_width="324dp"
            android:layout_height="57dp" android:id="@+id/saved_items_button"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="35dp"
            android:layout_marginEnd="52dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="25dp"
            app:layout_constraintTop_toBottomOf="@+id/personal_info_button" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="405dp" android:fontFamily="@font/amarante"/>
    <Button
            android:text="Personal Info"
            android:layout_width="324dp"
            android:layout_height="57dp" android:id="@+id/personal_info_button"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="35dp"
            android:layout_marginEnd="52dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="487dp"
            android:layout_marginTop="25dp" app:layout_constraintTop_toBottomOf="@+id/profile_info_button"
            android:fontFamily="@font/amarante"/>
    <TextView
            android:text="Settings"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView11"
            android:fontFamily="@font/amarante" android:textSize="34sp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="21dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="667dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="22dp"
            android:layout_marginEnd="272dp" app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="Profile Info"
            android:layout_width="324dp"
            android:layout_height="57dp" android:id="@+id/profile_info_button"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="35dp"
            android:layout_marginEnd="52dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="105dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="569dp"
            android:fontFamily="@font/amarante"/>
</androidx.constraintlayout.widget.ConstraintLayout>